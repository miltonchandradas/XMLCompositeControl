<mvc:View controllerName="com.sap.MyControls.controller.App" xmlns:mvc="sap.ui.core.mvc" displayBlock="true" xmlns="sap.m" xmlns:control="com.sap.MyControls.controls">
	<Shell id="shell">
		<App id="app">
			<pages>
				<Page id="page" title="{i18n>title}">
					<content>
						<control:Fibonacci id="fibControlByView" title="Fibonacci Series - Instantiated by View" next=".onHandleNext"></control:Fibonacci>
						<VBox id="placeholder"></VBox>
						<Button text="Reset Series" press=".onHandleReset" class="sapUiSmallMarginBottom"></Button>
					</content>
				</Page>
			</pages>
		</App>
	</Shell>
</mvc:View>